    "use-immer": "^0.11.0",
    "vite-plugin-commonjs": "^0.10.4",
    "ws": "^8.18.2",
    "zod": "^3.25.63",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/near-operation-file-preset": "^3.0.1",
