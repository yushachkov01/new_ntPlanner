    "graphql-tag": "^2.12.6",
    "graphql-ws": "^6.0.5",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-relay": "^19.0.0",
    "react-router-dom": "^6.30.0",
    "relay-runtime": "^19.0.0",
