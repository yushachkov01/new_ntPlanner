{
  "name": "corenoc",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "ws": "tsx -r dotenv/config src/ws-service.ts",
    "dev": "vite",
    "codegen": "dotenv -e .env -- graphql-codegen --config codegen.yml --watch",
    "build": "vite build",
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint . --ext .js,.jsx,.ts,.tsx --cache --config .eslintrc.cjs",
    "lint:fix": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint . --ext .js,.jsx,.ts,.tsx --fix --cache --config .eslintrc.cjs",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
